{
  "name": "@franekostrowski/country-utils",
  "repository": {
    "type": "git",
    "url": "git://github.com/franekostrowski/country-utils"
  },
  "version": "1.1.0",
  "license": "MIT",
  "author": "@franekostrowski",
  "main": "dist/index.js",
  "module": "dist/country-utils.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "dts build",
    "lint": "dts lint",
    "lint:fix": "dts lint --fix",
    "prepare": "dts build",
    "size": "size-limit",
    "start": "dts watch",
    "test": "dts test",
    "test:dev": "dts test --watch --verbose"
  },
  "husky": {
    "hooks": {
      "pre-commit": "dts lint"
    }
  },
  "prettier": {
    "singleQuote": false,
    "trailingComma": "none"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "engines": {
    "node": ">=12"
  },
  "size-limit": [
    {
      "path": "dist/country-utils.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/country-utils.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.1.0",
    "@tsconfig/recommended": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "dts-cli": "^1.6.3",
    "eslint": "^8.30.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.2",
    "size-limit": "^8.1.0",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4"
  }
}
